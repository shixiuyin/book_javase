{
	"title": "我的一本书",
	"author": "wwwshibapi",
	"description": "我第一本书的描述，很好",
	"language": "zh-hans",
	"structure": {
		"readme": "README.md"
	},
	"plugins": ["prism", "-highlight", "-lunr",
		"-sharing",
		"-search",
		"search-pro",
		"back-to-top-button",
		"chapter-fold",
		"expandable-chapters",
		"splitter",
		"page-treeview",
		"anchor-navigation-ex",
		"github",
		"edit-link",
		"popup",
		"lightbox",
		"hide-element",
		"theme-fexa",
		"tbfed-pagefooter",
		"donate",
		"copy-code-button",
		"emphasize",
		"klipse",
		"pageview-count",
		"auto-scroll-table"
	],
	"variables": {
		"themeFexa":{
			"nav":[
				{
					"url":"http://www.baidu.com",
					"target":"_blank",
					"name": "简易教程"
				},
				{
					"url":"http://www.baidu.com",
					"target":"_blank",
					"name": "API帮助文档"
				},{
					"url":"http://www.baidu.com",
					"target":"_blank",
					"name": "高级java"
				},{
					"url":"http://www.baidu.com",
					"target":"_blank",
					"name": "测试文档"
				}
				]
			}
	},
	"pluginsConfig": {
		"prism": {
			"css": [
				"syntax-highlighting/assets/css/prism/prism-xonokai.css"
			]
		},
		"theme-default": {
			"showLevel": true
		},
		"anchor-navigation-ex": {
			"isShowTocTitleIcon": true
		},
		"page-treeview": {
			"copyright": "Copyright &#169; aleen42",
			"minHeaderCount": "2",
			"minHeaderDeep": "2"
		},
		"anchor-navigation-ex": {
			"showLevel": false,
			"showGoTop": false
		},
		"github": {
			"url": "https://github.com/hzittest/book_javase"
		},
		"edit-link": {
			"base": "https://github.com/hzittest/book_javase/edit/master",
			"label": "编辑此页面"
		},
		"hide-element": {
			"elements": [".gitbook-link"]
		},
		"theme-fexa":{
			"search-placeholder":"输入关键字搜索",
			"logo":"content/images/logo.png",
			"favicon": "content/images/favicon.ico"
		},
		"tbfed-pagefooter": {
			"copyright":"",
			"modify_label": "该文件最后修改时间：",
			"modify_format": "YYYY-MM-DD HH:mm:ss"
		},
		"donate": {
			"button": "打赏",
			"alipayText": "支付宝打赏",
			"wechatText": "微信打赏",
			"alipay": "https://github.com/hzittest/book_javase/blob/master/content/images/alipay2.jpg?raw=true",
			"wechat": "https://github.com/shixiuyin/book_javase/blob/master/content/images/wecate.JPG?raw=true"
		}


	},
	"links": {
		"sidebar": {
			"个性链接1": "http://www.shibapi.com",
			"个性链接2": "https://www.baidu.com"
		}
	},
	"styles": {
		"website": "styles/website.css",
		"ebook": "styles/ebook.css",
		"pdf": "styles/pdf.css",
		"mobi": "styles/mobi.css",
		"epub": "styles/epub.css"
	}
}